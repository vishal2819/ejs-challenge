<%- include("partials/header") %>

<h1>Home</h1>
<p><%= startingContent %></p>

<div class="post-container">
  <% posts.forEach(function(post) { %>
  <div class="post">
    <h2><%= post.title %></h2>
    <p>
      <%= post.content.substring(0, 100) + '...' %>
      <a href="/posts/<%=post.title %>">Read More</a>
    </p>
  </div>
  <% }); %>
</div>

<%- include("partials/footer") %>